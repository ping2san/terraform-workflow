
name: Terraform plan DBS-DEV
on:
  push:
    branches:
      - development
      - main

jobs:
  Terraform:
    name: "Terraform"
    runs-on: ubuntu-latest
    environment:
      AWS_ACCESS_KEY_ID:
      AWS_SECRET_ACCESS_KEY:
      AWS_REGION: 'eu-west-2'

steps:
  - name: Checkout
    uses: actions/checkout@v2